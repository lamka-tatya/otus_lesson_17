{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "types/*": ["src/types/*"],
      "components/*": ["src/components/*"],
      "@/*": ["src/*"]
    },
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true
  }
}
